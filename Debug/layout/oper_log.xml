<?xml version="1.0" encoding="utf-8"?>
<UI>
  <group id="oper_log" text="LOG" size="800,600">
    <static  id="s_time_text" text="START_TIME" rel1="0.05,0" rel2="0.25,0.0" offset1="0,20" offset2="0,44"/>
    <datetime  id="s_time" type="date" rel1="0.25,0" rel2="0.4,0" offset1="0,20" offset2="0,44"/>
    <static  id="e_time_text" text="END_TIME" relto="s_time_text" offset1="0,40" offset2="0,40"/>
    <datetime  id="e_time" type="date" relto="s_time" offset1="0,40" offset2="0,40"/>
    <button  id="select" text="SELECT" rel1="0.6,0" rel2="0.8,0.0" offset1="0,40" offset2="0,64"/>
    <listctrl  id="log_list" topto="e_time_text BOTTOM" leftto="GROUP LEFT" rightto="GROUP RIGHT" bottomto="GROUP BOTTOM" margin="1,16,1,1">
      <column text="INDEX" width="100" format="left" sort_type="number"/>
      <column text="DATE" width="120" format="left" sort_type="date"/>
      <column text="TIME" width="120" format="left" sort_type="time"/>
      <column text="USER_NAME" width="120" format="left" sort_type="text"/>
      <column text="OPER_TYPE" width="120" format="left" sort_type="text"/>
      <column text="DETAIL" width="200" format="left" sort_type="text"/>
    </listctrl> 
  </group> 
</UI>